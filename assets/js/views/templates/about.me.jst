<div class="flex-boxes about-me">
  <div class="refills-wrapper">
    <div class="flex-boxes">
      <div class="flex-box">
        <img class="small" src="<%= image %> ">
      </div>
      <div>
        <div class="side-image-content">
          <h4><%= role %> - <%= company %> , <%= hometown %> </h4>
          <h1><%= name %>&nbsp;<%= surname %></h1>

          <div class="links-social">
            <a data-bypass href="<%= github %>" target="_blank"><img src="/assets/img/github.png"></a>
            <a data-bypass href="<%= twitter %>" target="_blank"><img src="/assets/img/twitter.png"></a>
            <a data-bypass href="<%= linkedin %>" target="_blank"><img src="/assets/img/linkedin.png"></a>
          </div>
          <hr>
          <p><%= description %></p>
          <p>&quot;<%= quote %>&quot;</p>
          <br>
          <br>
          <br>
          <h2>Education</h2>
          <% _.each(education, function(school, index){ %>
            <p><%= school.name %>: <%= school.years %></p>
            <% }); %>
            <h2>Skillset</h2>
            <% _.each(skills, function(skill, index){ %>
              <p><%= skill.name %> : <%= skill.level %> /10</p>
              <% }); %>
              <br>
              <br>
              <br>
              <a data-bypass href="http://itworkslocal.ly/" target="_blank">
                <img src="/assets/img/gallery/iwlFooter.png" alt="Logo image">
              </a>
            </div>
          </div>
